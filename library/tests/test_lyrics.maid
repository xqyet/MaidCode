func printLyrics() {
    # Each item: [line text, per-char delay, pause after line]
    obj lines = [
        ["This is one test", 0.06, 1.33],
        ["This is two test", 0.07, 0.33],
        ["This is three test", 0.06, 0.33]
    ];

    walk li = 0 through length(lines) {
        obj entry = retrieve(lines, li);
        obj text        = retrieve(entry, 0);
        obj char_delay  = retrieve(entry, 1);
        obj after_delay = retrieve(entry, 2);

        obj out = "";
        obj i = 0;

        while i < length(text) {
            obj out = out + charat(text, i);
            inline("\r" + out);     # overwrite same line
            rest(char_delay);      # per-char delay
            obj i = i + 1;
        }

        inline("\n");               # end the line

        if after_delay > 0 {
            rest(after_delay);     # pause before next
        }
    }
}

# Call the function
printLyrics();
